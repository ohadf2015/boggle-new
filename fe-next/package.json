{
  "name": "fe-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "build:mobile": "NEXT_PUBLIC_IS_MOBILE=true next build && npx cap sync",
    "build:mobile:ios": "NEXT_PUBLIC_IS_MOBILE=true next build && npx cap sync ios",
    "build:mobile:android": "NEXT_PUBLIC_IS_MOBILE=true next build && npx cap sync android",
    "start": "node server.js",
    "lint": "eslint",
    "stress": "node scripts/stress-test.js --clients=100 --duration=30",
    "db:migrate": "node supabase/migrations/run-migrations.js",
    "db:migrate:check": "node -e \"require('./supabase/migrations/run-migrations.js')\" --dry-run",
    "postbuild": "npm run db:migrate || echo 'Migration skipped (missing service key)'",
    "cap:init": "npx cap init LexiClash com.lexiclash.app --web-dir out",
    "cap:add:ios": "npx cap add ios",
    "cap:add:android": "npx cap add android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:sync": "npx cap sync",
    "cap:copy": "npx cap copy"
  },
  "dependencies": {
